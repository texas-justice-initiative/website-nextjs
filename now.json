{
  "version": 2,
  "name": "tji-nextjs",
  "public": false,
  "alias": ["tji-nextjs"],
  "env": {
    "SENDGRID_API_KEY": "@tji-sendgrid-key",
    "PAYPAL_SANDBOX_API_KEY": "@tji-paypal-sandbox-key",
    "PAYPAL_LIVE_API_KEY": "@tji-paypal-live-key"
  },
  "builds": [
    { "src": "next.config.js", "use": "@now/next" },
    { "src": "/static/*", "use": "@now/static" },
    { "src": "/api/**/*.js", "use": "@now/node-server" }
  ],
  "routes": [
    {
      "src": "/api/contact",
      "methods": ["POST"]
    },
    {
      "src": "/_next/static/(?:[^/]+/pages|chunks|runtime)/.+",
      "headers": { "cache-control": "immutable" }
    }
  ]
}
